#
#
# the read tests
#
#
# read on an empty file
#
# read 0
open /tmp_dir_0/d1/nonempty_dir1/f1.txt 0
pread (FD 3) 0 0
close (FD 3)
# read 1
open /tmp_dir_0/d1/nonempty_dir1/f1.txt 0
pread (FD 3) 1 0
close (FD 3)
# read beyond end
open /tmp_dir_0/d1/nonempty_dir1/f1.txt 0
pread (FD 3) 1 100
close (FD 3)
# read negative
open /tmp_dir_0/d1/nonempty_dir1/f1.txt 0
pread (FD 3) 1 -100
close (FD 3)
# check the size of the file
stat /tmp_dir_0/d1/nonempty_dir1/f1.txt
#
#
# read on a nonempty file
#
# f3.txt is 23 bytes long
# read 0
open /tmp_dir_0/d1/nonempty_dir1/d2/f3.txt 0
pread (FD 3) 0 0
close (FD 3)
# read at end
open /tmp_dir_0/d1/nonempty_dir1/d2/f3.txt 0
pread (FD 3) 1 23
close (FD 3)
# read beyond end
open /tmp_dir_0/d1/nonempty_dir1/d2/f3.txt 0
pread (FD 3) 1 100
close (FD 3)
# read over end
open /tmp_dir_0/d1/nonempty_dir1/d2/f3.txt 0
pread (FD 3) 5 20
close (FD 3)
# read negative
open /tmp_dir_0/d1/nonempty_dir1/d2/f3.txt 0
pread (FD 3) 0 -100
pread (FD 3) 1 -100
close (FD 3)
# check size of file
stat /tmp_dir_0/d1/nonempty_dir1/d2/f3.txt
#
#
# read on a directory
# 
open /tmp_dir_0/d1/empty_dir1 0
pread (FD 3) 0 0
pread (FD 3) 1 0
pread (FD 3) 1 1
close (FD 3)
open /tmp_dir_0/d1/nonempty_dir1 0
pread (FD 3) 0 0
pread (FD 3) 1 0
pread (FD 3) 1 1
close (FD 3)
dump